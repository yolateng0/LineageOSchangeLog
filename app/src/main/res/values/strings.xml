    <string name="info_content" formatted="false">
        <![CDATA[
        	This app shows every merged change on the Custom ROM Gerrit instance after your current build time.<br /><br />
        	<b>Current build time:</b><br />
        	<font color="#444444">%buildtime</font><br /><br />
        	<b>Last refresh of list:</b><br />
        	<font color="#444444">%lastrefresh</font><br /><br />
        	<b>Copyright:</b><br />
        	<font color="#444444">ByteHamster</font>
        ]]>
    </string>
    <string name="first_warning_message" formatted="false">
        <![CDATA[
        	The dates in this list show when the changes were <b>merged</b>. <br/><br/>
        	They will be included in the <b>next</b> nightly, usually <b>one day later</b>.
        ]]>
    </string>
